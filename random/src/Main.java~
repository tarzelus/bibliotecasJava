import java.util.*;

public class Main {

	public static void main(String[] args) 
	{
		
		int num1;

		System.out.print("\nImprimira un numero aleatorio y dira si el numero es par o impar ");
		System.out.print("\n ");
		
	
		Random rndm = new Random ();
		
		num1=rndm.setSeed(99); //rndm.nextInt(100);	
		
		
		System.out.print("\n el numero creado aleatoriamente:  "+ num1  + "  ");

		if ((num1%2)==0)	
		{
			System.out.print("\n el numero creado es par \n ");
		}	
		else 
		{
			System.out.print("\n el numero creado es impar \n");

		}
				
	}
}



		

